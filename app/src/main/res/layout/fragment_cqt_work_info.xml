    <TextView
        android:id="@+id/tv_3"
        style="@style/cqt_fragment_workInfo"
        android:text="成功次数:"/>
    <TextView
        android:id="@+id/tv_4"
        style="@style/cqt_fragment_workInfo"
        android:text="失败次数:"/>
    <TextView
        android:id="@+id/tv_5"
        style="@style/cqt_fragment_workInfo"
        android:text="失败次数:"
        />
    <TextView
        android:id="@+id/tv_6"
        style="@style/cqt_fragment_workInfo"
