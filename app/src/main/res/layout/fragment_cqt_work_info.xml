              android:layout_height="230dp"
              android:background="@color/cqt_data_background"
              android:orientation="vertical">
    <TextView
        android:id="@+id/tv_0"
        android:background="@drawable/shape_test_info_tvbg"
        android:paddingLeft="18dp"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:text="ATTACH/DETACH:"/>
    <TextView
        android:id="@+id/tv_1"
        style="@style/cqt_fragment_workInfo"
        android:text="ATTACH/DETACH:"/>
    <TextView
        android:id="@+id/tv_2"
        style="@style/cqt_fragment_workInfo"
