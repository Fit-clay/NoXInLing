<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_marginBottom="4dp"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">
    <include
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/top_bar_leftback_righttext"
        />
    <LinearLayout
        android:layout_marginTop="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"

        >
        <TextView
            android:text="测试名称"
            android:layout_width="0dp"
            android:layout_weight="1.3"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            />
        <EditText
            android:id="@+id/tv_model_name"
            android:layout_width="0dp"
            android:layout_weight="3.25"
            android:layout_marginRight="12dp"
            android:layout_height="wrap_content"
            android:background="@color/basic_message_collection_background" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_testmodel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:orientation="horizontal"

        >

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_weight="1.3"
            android:text="测试模式" />

        <Spinner
            android:id="@+id/sp_test_model"
            style="@style/work_spinnerstyle"
            android:layout_marginRight="12dp"
            android:layout_weight="3.25"></Spinner>

    </LinearLayout>
    <LinearLayout

        android:id="@+id/ll_model_detail_testTime"
        android:layout_marginTop="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"

        >
        <TextView
            android:id="@+id/tv_model_testTime"
            android:text="测试时长(S)"
            android:layout_width="0dp"
            android:layout_weight="1.3"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            />
        <EditText
            android:id="@+id/et_model_testTime"
            android:layout_width="0dp"
            android:layout_weight="3.25"
            android:layout_marginRight="12dp"
            android:layout_height="wrap_content"
            android:background="@color/basic_message_collection_background" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_model_times"
        android:layout_marginTop="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"

        >
        <TextView
            android:id="@+id/tv_test_time"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_weight="1.3"
            android:text="测试次数" />

        <EditText
            android:id="@+id/tv_model_times"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="12dp"
            android:layout_weight="3.25"
            android:background="@color/basic_message_collection_background" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_tv_model_circleTime"
        android:layout_marginTop="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"

        >

        <TextView
            android:id="@+id/textView7"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_weight="1.3"
            android:text="测试间隔(s)" />

        <EditText
            android:id="@+id/et_model_circleTime"
            android:layout_width="0dp"
            android:layout_weight="3.25"
            android:layout_marginRight="12dp"
            android:layout_height="wrap_content"
            android:background="@color/basic_message_collection_background" />
    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <include
            layout="@layout/menu_line2">
        </include>
    </LinearLayout>
    <!--Wait-->


    <!--语音-->
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/ll_voice_call"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <LinearLayout
        android:layout_marginTop="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:text="呼叫方式"
            android:layout_width="0dp"
            android:layout_weight="1.3"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            />
        <Spinner
            android:layout_marginRight="12dp"
            android:layout_weight="3.25"

            android:id="@+id/call_model"
            style="@style/work_spinnerstyle"></Spinner>
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"

        >
        <TextView
            android:id="@+id/tv_number"
            android:text="被叫号码"
            android:layout_width="0dp"
            android:layout_weight="1.3"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            />

        <EditText
            android:id="@+id/et_model_detail_callNumber"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="12dp"
            android:layout_weight="3.25"
            android:background="@color/basic_message_collection_background" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_model_durationTime"
        android:layout_marginTop="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"

        >
        <TextView
            android:text="保持时间"
            android:layout_width="0dp"
            android:layout_weight="1.3"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            />
        <EditText
            android:id="@+id/et_model_durationTime"
            android:layout_width="0dp"
            android:layout_weight="3.25"
            android:layout_marginRight="12dp"
            android:layout_height="wrap_content"
            android:background="@color/basic_message_collection_background" />
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"

        >
        <TextView
            android:text="阻塞时间"
            android:layout_width="0dp"
            android:layout_weight="1.3"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            />
        <EditText
            android:id="@+id/et_model_blockTime"
            android:layout_width="0dp"
            android:layout_weight="3.25"
            android:layout_marginRight="12dp"
            android:layout_height="wrap_content"
            android:background="@color/basic_message_collection_background" />
    </LinearLayout>

    </LinearLayout>
<!--FTP-->
    <LinearLayout
        android:id="@+id/ll_ftp"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.3"
                android:text="线程数量 " />

            <EditText
                android:id="@+id/et_model_nthreadNum"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="3.25"
                android:background="@color/basic_message_collection_background" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal"

            >

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.3"
                android:text="停转超时(S)" />

            <EditText
                android:id="@+id/et_model_noDataTime"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="3.25"
                android:background="@color/basic_message_collection_background" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal"

            >

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.3"
                android:text="执行超时(S)" />

            <EditText
                android:id="@+id/et_model_outTime"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="3.25"
                android:background="@color/basic_message_collection_background" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal"

            >

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.3"
                android:text="服务器路径" />

            <EditText
                android:id="@+id/et_model_connectPath"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="3.25"
                android:background="@color/basic_message_collection_background" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal"

            >

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.3"
                android:text="文件选择" />

            <Spinner
                android:id="@+id/sp_ftp_dcoum"
                style="@style/work_spinnerstyle"
                android:layout_marginRight="12dp"
                android:layout_weight="3.25"></Spinner>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal"

            >

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.3"
                android:text="服务器地址" />

            <EditText
                android:id="@+id/et_model_connectIp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="3.25"
                android:background="@color/basic_message_collection_background" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal"

            >

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.3"
                android:text="用户名" />

            <EditText
                android:id="@+id/et_model_userName"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="3.25"
                android:background="@color/basic_message_collection_background" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal"

            >

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.3"
                android:text="密码" />

            <EditText
                android:id="@+id/tv_model_password"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="3.25"
                android:background="@color/basic_message_collection_background" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal"

            >

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.3"
                android:text="FTP模式" />

            <Spinner
                android:id="@+id/sp_ftp_model"
                style="@style/work_spinnerstyle"
                android:layout_marginRight="12dp"
                android:layout_weight="3.25"></Spinner>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal"

            >

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.3"
                android:text="端口" />

            <EditText
                android:id="@+id/et_model_port"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="3.25"
                android:background="@color/basic_message_collection_background" />
        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_marginBottom="2dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></LinearLayout>
    <LinearLayout
        android:id="@+id/ll_http"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.3"
                android:text="目标名称" />

            <EditText
                android:id="@+id/ed_corver_target_http"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="3.25"
                android:background="@color/basic_message_collection_background" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.3"
                android:text="目标url" />

            <EditText
                android:id="@+id/ed_corver_target_http_url"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="3.25"
                android:background="@color/basic_message_collection_background" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_ping"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.3"
                android:text="目标名称" />

            <EditText
                android:id="@+id/ed_corver_target"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="3.25"
                android:background="@color/basic_message_collection_background" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.3"
                android:text="目标url" />

            <EditText
                android:id="@+id/ed_corver_target_url"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="3.25"
                android:background="@color/basic_message_collection_background" />

        </LinearLayout>

    </LinearLayout>


</LinearLayout>
